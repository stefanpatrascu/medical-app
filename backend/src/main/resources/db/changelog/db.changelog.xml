<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
  http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.4.xsd">
  <changeSet id="insert_users" author="me">
    <preConditions>
      <not>
        <changeSetExecuted id="insert_users" author="me" />
      </not>
    </preConditions>
    <loadData
      encoding="UTF-8"
      file="classpath:db/changelog/csv/users.csv"
      separator=","
      tableName="users">
      <column name="first_name" type="STRING"/>
      <column name="last_name" type="STRING"/>
      <column name="password" type="STRING"/>
      <column name="email" type="STRING"/>
      <column name="role" type="STRING"/>
    </loadData>
  </changeSet>
  <changeSet author="me" id="populate-medical-specializations">
    <preConditions>
      <not>
        <changeSetExecuted id="populate-medical-specializations" author="me" />
      </not>
    </preConditions>
    <loadData
      encoding="UTF-8"
      file="classpath:db/changelog/csv/medical_specializations.csv"
      separator=","
      tableName="medical_specializations">
      <column name="label" type="STRING"/>
    </loadData>
  </changeSet>
</databaseChangeLog>
